{
  "version": 2,
  "builds": [
    {
      "src": "ecommerce/wsgi_vercel.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ecommerce/wsgi_vercel.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "ecommerce.settings"
  }
}